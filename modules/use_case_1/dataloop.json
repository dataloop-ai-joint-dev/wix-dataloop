{
  "displayName": "Use Case 1 CSV Converter",
  "name": "use_case_1",
  "scope": "project",
  "version": "0.0.1",
  "description": "Converts Use Case 1 CSV files to Prompt Items",
  "attributes": {
    "Category": "Application",
    "Provider": "Dataloop",
    "Deployed By": "Dataloop",
    "Application Type": [
      "Pipeline Node"
    ],
    "Media Type": [
      "Text"
    ]
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-joint-dev/wix-dataloop",
    "gitTag": "0.0.1"
  },
  "components": {
    "services": [
      {
        "name": "use-case-1",
        "moduleName": "use_case_1_module",
        "versions": {
          "dtlpy": "1.99.7"
        },
        "runtime": {
          "podType": "regular-xs",
          "runnerImage": "dataloopai/dtlpy-agent:cpu.py3.10.opencv",
          "concurrency": 1,
          "autoscaler": {
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 10
          }
        }
      }
    ],
    "pipelineNodes": [
      {
        "invoke": {
          "type": "function",
          "namespace": "use-case-1.use_case_1_module.process_csv_use_case_1"
        },
        "name": "process_csv_use_case_1",
        "categories": [],
        "displayName": "Use Case 1 CSV Converter",
        "description": "Converting Use Case 1 CSVs Converter into prompt items",
        "scope": "project",
        "configuration": {
          "fields": [
            {
              "name": "name",
              "title": "Node Name",
              "props": {
                "title": true,
                "type": "string",
                "default": "Use Case 1",
                "required": true,
                "placeholder": "Insert node name"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            }
          ]
        }
      }
    ],
    "modules": [
      {
        "name": "use_case_1_module",
        "computeConfig": "process-csv-use-case-1",
        "entryPoint": "modules/use_case_1/main.py",
        "className": "ServiceRunner",
        "initInputs": [],
        "functions": [
          {
            "name": "process_csv_use_case_1",
            "input": [
              {
                "type": "Item",
                "name": "item"
              }
            ],
            "output": [],
            "displayIcon": "icon-dl-overview",
            "displayName": "Use Case 1 CSV Converter"
          }
        ]
      }
    ],
    "triggers": [
      {
        "name": "use-case-1-on-item",
        "active": true,
        "type": "Event",
        "namespace": "services.use-case-1",
        "spec": {
          "filter": {
            "$and": [
              {
                "metadata.system.mimetype": "text/csv",
                "datasetId": "673ce1b8c753fd33062d900e"
              },
              {
                "hidden": false
              },
              {
                "type": "file"
              }
            ]
          },
          "executionMode": "Always",
          "resource": "Item",
          "actions": [
            "Created"
          ],
          "input": {},
          "operation": {
            "type": "function",
            "functionName": "process_csv_use_case_1"
          }
        }
      }
    ]
  }
}